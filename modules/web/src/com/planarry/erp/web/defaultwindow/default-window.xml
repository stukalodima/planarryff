        caption="msg://caption"
        class="com.planarry.erp.web.defaultwindow.DefaultWindow"
        messagesPack="com.planarry.erp.web.defaultwindow">
    <dialogMode/>
    <layout>
        <accordion id="accordionTM"
                   align="TOP_CENTER"
                   responsive="true"
                   width="930px">
            <tab id="tabTransports"
                 visible="false"
                 caption="msg://Transport"
                 spacing="true">
                <cssLayout id="TransportCssLayout"
                           stylename="card"
                           width="100%">
                    <hbox align="MIDDLE_CENTER"
                          margin="true"
                          stylename="well"
                          width="100%">
                        <vbox stylename="well">
                            <vbox align="MIDDLE_CENTER"
                                  margin="true"
                                  spacing="true"
                                  responsive="true"
                                  stylename="card"
                                  width="300px">
                                <button id="transportListBtn"
                                        align="MIDDLE_CENTER"
                                        height="100px"
                                        icon="icon/truck.png"
                                        invoke="onTransportListBtnClick"
                                        responsive="true"
                                        stylename="quiet"
                                        width="100%"/>
                                <label id="label1"
                                       align="MIDDLE_CENTER"
                                       stylename="h2"
                                       value="msg://TransportList"/>
                            </vbox>
                            <vbox align="MIDDLE_CENTER"
                                  margin="true"
                                  responsive="true"
                                  spacing="true"
                                  stylename="card"
                                  width="300px">
                                <button id="TransportPlaceBtn"
                                        align="MIDDLE_CENTER"
                                        height="100px"
                                        icon="icon/transportPlace.png"
                                        invoke="onTransportPlaceBtnClick"
                                        responsive="true"
                                        stylename="quiet"
                                        width="100%"/>
