                                       align="MIDDLE_CENTER"
                                       stylename="h2"
                                       value="msg://TransportPlace"/>
                            </vbox>
                        </vbox>
                        <vbox stylename="well">
                            <vbox align="TOP_CENTER"
                                  margin="true"
                                  responsive="true"
                                  spacing="true"
                                  stylename="card"
                                  width="300px">
                                <button id="TrailerListBtn"
                                        align="MIDDLE_CENTER"
                                        height="100px"
                                        icon="icon/trailer.png"
                                        invoke="onTrailerListBtnClick"
                                        responsive="true"
                                        stylename="quiet"
                                        width="100%"/>
                                <label id="label"
                                       align="MIDDLE_CENTER"
                                       stylename="h2"
                                       value="msg://TrailerList"/>
                            </vbox>
                            <vbox align="TOP_CENTER"
                                  margin="true"
                                  responsive="true"
                                  spacing="true"
                                  stylename="card"
                                  width="300px">
                                <button id="categoryListBtn"
                                        align="MIDDLE_CENTER"
                                        height="100px"
