                        <vbox stylename="well">
                            <vbox align="TOP_CENTER"
                                  margin="true"
                                  responsive="true"
                                  spacing="true"
                                  stylename="card"
                                  width="300px">
                                <button id="TransportManagersBtn"
                                        align="MIDDLE_CENTER"
                                        height="100px"
                                        icon="icon/transport_managers.png"
                                        invoke="onTransportManagersBtnClick"
                                        responsive="true"
                                        stylename="quiet"
                                        width="100%"/>
                                <label id="TransportManagerLbl"
                                       align="MIDDLE_CENTER"
                                       stylename="h2"
                                       value="msg://TransportManager"/>
                            </vbox>
                        </vbox>
                    </hbox>
                </cssLayout>
            </tab>

